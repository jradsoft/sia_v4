<Window x:Class="wpfEFac.Views.Paises.EditarPais"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="EditarPais" Height="150" Width="300">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="80"/>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="44"/>
            <RowDefinition Height="15"/>
            <RowDefinition Height="47*"/>
        </Grid.RowDefinitions>
        <TextBlock Height="23" Grid.Row="0" HorizontalAlignment="Right" Name="txbNombre" Text="Nombre: " VerticalAlignment="Center" />
        <TextBox Height="23" Grid.Row="0" Grid.Column="1" HorizontalAlignment="Left" Name="txtNombre" Margin="5,0,0,0" VerticalAlignment="Center" Width="170" />
        <Button Name="bttGuardar" Margin="0,0,113,24"  Content="Guardar" Grid.Row="2" Grid.Column="1" HorizontalAlignment="Right" Width="80" Height="23" Click="bttGuardar_Click"/>
        <Button Name="bttCancelar" Margin="0,0,23,24" Content="Cancelar" Grid.Row="2" Grid.Column="1" HorizontalAlignment="Right" Width="80" Height="23" Click="bttCancelar_Click"/>        
    </Grid>
</Window>
